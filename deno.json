{
  "workspace": ["./datacontracts", "config-provider", "./db", "app-context", "repos", "./server", "./user-clients" ],
  "tasks": {
    "dev": "deno run --watch main.ts",
    "start": "deno run -A ./server/main.ts",
    "publish": "deno run -A ./scripts/publishAll.ts",
    "publish-by-name": "deno run -A ./scripts/publishByName.ts"
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "@std/assert": "jsr:@std/assert@1",
    "chalk": "npm:chalk@^5.3.0"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx",
    "strict": true
  }
}
